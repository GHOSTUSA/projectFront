  "shortcuts": [
    {
      "name": "Voir les restaurants",
      "short_name": "Restaurants",
      "description": "Parcourir la liste des restaurants disponibles",
      "url": "/utilisateur/restaurant",
      "icons": [{ "src": "/icons/restaurants-icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Mon panier",
      "short_name": "Panier",
      "description": "Voir mon panier de commande",
      "url": "/utilisateur/panier",
      "icons": [{ "src": "/icons/cart-icon.png", "sizes": "96x96" }]
    },
    {
      "name": "Mon compte",
      "short_name": "Compte",
      "description": "Gérer mon compte utilisateur",
      "url": "/utilisateur/compte",
      "icons": [{ "src": "/icons/account-icon.png", "sizes": "96x96" }]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/mobile-home.png",
      "sizes": "540x720",
      "type": "image/png",
      "platform": "narrow",
      "label": "Page d'accueil sur mobile"
    },
    {
      "src": "/screenshots/desktop-restaurants.png",
      "sizes": "1280x800",
      "type": "image/png",
      "platform": "wide",
      "label": "Liste des restaurants sur desktop"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+fooddelivery",
      "url": "/restaurant/%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
